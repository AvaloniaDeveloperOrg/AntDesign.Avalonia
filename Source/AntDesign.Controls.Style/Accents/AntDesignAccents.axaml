<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:antdesign="using:AntDesign.Controls"
					xmlns:antdesignconverters="using:AntDesign.Controls.Converters"
					x:CompileBindings="True">
	
	<antdesignconverters:BooleansAndConverter x:Key="BooleansAndConverter"/>
  <antdesignconverters:Double2ScaleTransformConverter x:Key="Double2ScaleTransformConverter"/>

  <!-- Drawer -->
	<x:Double x:Key="DrawerMaskOpacity">1</x:Double>
	
	<!-- AntDesignTreeView -->
	<GridLength x:Key="AntDesignTreeViewInnerWidth">3</GridLength>
	
</ResourceDictionary>
