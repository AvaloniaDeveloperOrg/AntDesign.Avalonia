<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:CompileBindings="True">
	<!-- Drawer -->
	<BoxShadows x:Key="DrawerPanelBoxShadow">0 0 6 1 Gray</BoxShadows>

	<SolidColorBrush x:Key="DrawerPanelBackground" Color="{StaticResource AntDesignBackgroundColor}"/>
	<SolidColorBrush x:Key="DrawerMaskBackground" Color="Black" Opacity="0.2"/>

	<!-- Panel -->
	<SolidColorBrush x:Key="SideMenuBackground" Color="{StaticResource AntDesignBackgroundColor}"/>
	<SolidColorBrush x:Key="TopHeaderBackground" Color="{StaticResource AntDesignBackgroundColor}"/>

	<SolidColorBrush x:Key="TopHeaderMixBackground" Color="RGB(0,21,41)"/>
	<SolidColorBrush x:Key="TopHeaderMixForeground" Color="White"/>
	<SolidColorBrush x:Key="SideMenuMixBackground" Color="{StaticResource AntDesignBackgroundColor}"/>

	<SolidColorBrush x:Key="TopHeaderTopBackground" Color="{StaticResource AntDesignBackgroundColor}"/>
	
	<SolidColorBrush x:Key="SideMenuSideBackground" Color="{StaticResource AntDesignBackgroundColor}"/>


	<BoxShadows x:Key="TopHeaderBoxShadow">0 0 6 1 LightGray</BoxShadows>
	<BoxShadows x:Key="SideMenuBoxShadow">0 3 6 1 LightGray</BoxShadows>


</ResourceDictionary>
