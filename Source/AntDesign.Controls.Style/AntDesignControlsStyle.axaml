<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		x:Class="AntDesign.AntDesignControlsStyle"
		x:CompileBindings="True">
	<Styles.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>
				<ResourceInclude x:Key="Light" Source="avares://AntDesign.Controls.Style/Accents/AntDesignLight.axaml"/>
				<ResourceInclude x:Key="Dark"  Source="avares://AntDesign.Controls.Style/Accents/AntDesignDark.axaml"/>
			</ResourceDictionary.ThemeDictionaries>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://AntDesign.Controls.Style/Styles/AntDesignControls.axaml" />
				<ResourceInclude Source="avares://AntDesign.Controls.Style/Accents/AntDesignAccents.axaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Styles.Resources>
</Styles>
