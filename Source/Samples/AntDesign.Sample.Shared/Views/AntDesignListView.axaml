<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AntDesign.Sample.Views.AntDesignListView">

	<Grid RowDefinitions="Auto,5,50">
		<AntDesignTreeView HorizontalAlignment="Left"
					   WidthBeforeClosing="200" WidthAfterClosing="20"
					   IsPanelExpanded="{Binding IsTreeViewExpanded}">
			<AntDesignTreeViewItem Header="123">
				<AntDesignTreeViewItem Header="2323"/>
				<AntDesignTreeViewItem Header="4455"/>
			</AntDesignTreeViewItem>
		</AntDesignTreeView>
		
		
		<Button Grid.Row="2" Content="Click" Command="{Binding TriggerClickCommand}"/>
		
	</Grid>
	
	
</UserControl>
