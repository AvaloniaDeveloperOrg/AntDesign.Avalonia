<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<double x:Key="DataGridHeaderFontSize">15</double>
	<double x:Key="DataGridHeaderHeight">45</double>
	<FontWeight x:Key="DataGridHeaderFontWeight">Bold</FontWeight>
	<Thickness x:Key="DataGridHeaderPadding">12,0,0,0</Thickness>
	<Thickness x:Key="DataGridSeparatorMargin">0,4,0,4</Thickness>

	<Thickness x:Key="DataGridBorderThickness">1</Thickness>
	<CornerRadius x:Key="DataGridCornerRadius">10,10,0,0</CornerRadius>
	<double x:Key="DataGridScrollBarSize">16</double>
	
	
	
</ResourceDictionary>
