<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="AntDesignDataGridBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridForeground" Color="Black" Opacity="0.9"/>
	<SolidColorBrush x:Key="AntDesignDataGridBorderBrush" Color="#d9d9d9"/>

	<SolidColorBrush x:Key="AntDesignDataGridHeaderBackground" Color="#fafafa"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderPointerOverBackground" Color="#f0f0f0"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderPressedBackground" Color="#f0f0f0"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderForeground" Color="Black"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderBorderBrush" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderSeparatorBrush" Color="#d9d9d9"/>

	<SolidColorBrush x:Key="AntDesignDataGridCellBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridCellBorderBrush" Color="Transparent"/>
	
	<SolidColorBrush x:Key="AntDesignDataGridRowBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridRowPointerOverBackground" Color="#fafafa"/>
	<SolidColorBrush x:Key="AntDesignDataGridRowSelectedBackground" Color="#e6f4ff"/>
	<SolidColorBrush x:Key="AntDesignDataGridRowSelectedPointerOverBackground" Color="#bae0ff"/>


</ResourceDictionary>
