	<SolidColorBrush x:Key="AntDesignDataGridHeaderIconPrimaryBackground" Color="#FF1668DC"/>
	<SolidColorBrush x:Key="AntDesignDataGridIndicatorTemplateBackground" Color="#FF1668DC"/>


</ResourceDictionary>
