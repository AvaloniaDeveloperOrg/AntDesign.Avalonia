	<SolidColorBrush x:Key="AntDesignDataGridCellBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridCellBorderBrush" Color="Transparent"/>
	
	<SolidColorBrush x:Key="AntDesignDataGridRowBackground" Color="Transparent"/>
