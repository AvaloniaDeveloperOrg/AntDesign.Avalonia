	<SolidColorBrush x:Key="AntDesignDataGridRowDetailBackground" Color="#1d1d1d"/>
	<SolidColorBrush x:Key="AntDesignDataGridRowFocusVisualBorderBrush" Color="#217346" Opacity="0.8"/>
