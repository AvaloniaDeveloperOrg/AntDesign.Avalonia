<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="AntDesignDataGridBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AntDesignDataGridForeground" Color="White" Opacity="0.9"/>
	<SolidColorBrush x:Key="AntDesignDataGridBorderBrush" Color="#313131"/>

	<SolidColorBrush x:Key="AntDesignDataGridHeaderBackground" Color="#1d1d1d"/>
	<SolidColorBrush x:Key="AntDesignDataGridHeaderPointerOverBackground" Color="#303030"/>
