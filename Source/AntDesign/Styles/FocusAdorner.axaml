<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<FocusAdornerTemplate x:Key="{x:Type FocusAdornerTemplate}">
		<Border BorderThickness="{DynamicResource FocusAdornerFocusVisualPrimaryThickness}"
				BorderBrush="{DynamicResource FocusAdornerFocusVisualPrimaryBrush}"
				Margin="{DynamicResource FocusAdornerFocusVisualMargin}">
			<Border BorderThickness="{DynamicResource FocusAdornerFocusVisualSecondaryThickness}"
					BorderBrush="{DynamicResource FocusAdornerFocusVisualSecondaryBrush}" />
		</Border>
	</FocusAdornerTemplate>
	

</ResourceDictionary>
