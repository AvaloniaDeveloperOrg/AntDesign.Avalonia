<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
					x:CompileBindings="True">
	<ControlTheme x:Key="{x:Type ThemeVariantScope}" TargetType="ThemeVariantScope">
		<Setter Property="TextElement.FontSize" Value="{DynamicResource FontSize}"/>
		<Setter Property="TextElement.FontFamily" Value="{DynamicResource AntDesignFontFamily}"/>
		<Setter Property="TextElement.Foreground" Value="{DynamicResource AntDesignForeground}"/>
		<Setter Property="Padding" Value="{DynamicResource ThemeVariantScopePadding}"/>
	</ControlTheme>
</ResourceDictionary>
