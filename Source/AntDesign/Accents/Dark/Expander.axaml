<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">
	
	<SolidColorBrush x:Key="ExpanderBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ExpanderForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ExpanderBorderBrush" Color="{StaticResource AntDesignBorderColor}"/>

	<SolidColorBrush x:Key="ExpanderHeaderBackground" Color="{DynamicResource AntDesignAssistColor4}"  Opacity="0.05"/>
	<SolidColorBrush x:Key="ExpanderHeaderForeground" Color="{StaticResource AntDesignForegroundColor}"/>

	<SolidColorBrush x:Key="ExpanderHeaderPointerOverBackground" Color="{DynamicResource AntDesignAssistColor4}"  Opacity="0.05"/>
	<SolidColorBrush x:Key="ExpanderHeaderPointerOverForeground" Color="{StaticResource AntDesignForegroundColor}"/>

	<SolidColorBrush x:Key="ExpanderHeaderPressedBackground" Color="{DynamicResource AntDesignAssistColor4}"  Opacity="0.05"/>
	<SolidColorBrush x:Key="ExpanderHeaderPressedForeground" Color="{StaticResource AntDesignForegroundColor}"/>

	<SolidColorBrush x:Key="ExpanderHeaderSelectedBackground" Color="{DynamicResource AntDesignAssistColor4}"  Opacity="0.05"/>
	<SolidColorBrush x:Key="ExpanderHeaderSelectedForeground" Color="{StaticResource AntDesignForegroundColor}"/>

</ResourceDictionary>
