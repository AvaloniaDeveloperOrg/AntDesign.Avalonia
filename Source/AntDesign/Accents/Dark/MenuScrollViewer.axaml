<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="MenuScrollViewerIconFill" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="MenuScrollViewerIconPointerOverFill" Color="{DynamicResource AntDesignSecondlyColor}"/>
	<SolidColorBrush x:Key="MenuScrollViewerIconPressedFill" Color="{DynamicResource AntDesignThirdlyColor}"/>

	<SolidColorBrush x:Key="MenuScrollViewerIconPointerOverBackground" Color="{DynamicResource AntDesignAssistColor2}"/>
	<SolidColorBrush x:Key="MenuScrollViewerIconPressedBackground" Color="{DynamicResource AntDesigndAssistColor5}"/>
 
</ResourceDictionary>
