<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">
	
	<SolidColorBrush x:Key="RepeatButtonBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="RepeatButtonForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="RepeatButtonBorderBrush" Color="{StaticResource AntDesignBorderColor}"/>

	<SolidColorBrush x:Key="RepeatButtonPointerOverBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="RepeatButtonPointerOverForeground" Color="{DynamicResource AntDesignSecondlyColor}"/>
	<SolidColorBrush x:Key="RepeatButtonPointerOverBorderBrush" Color="{DynamicResource AntDesignSecondlyColor}"/>

	<SolidColorBrush x:Key="RepeatButtonPressedBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="RepeatButtonPressedForeground" Color="{DynamicResource AntDesignThirdlyColor}"/>
	<SolidColorBrush x:Key="RepeatButtonPressedBorderBrush" Color="{DynamicResource AntDesignThirdlyColor}"/>
	
</ResourceDictionary>
