<Style xmlns="https://github.com/avaloniaui"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Style.Resources>
		<!--Global-->
		<SolidColorBrush x:Key="PrimaryBrush" Color="{DynamicResource PrimaryColor}"/>
		<SolidColorBrush x:Key="SecondlyBrush" Color="{DynamicResource SecondlyColor}"/>
		<SolidColorBrush x:Key="ThirdlyBrush" Color="{DynamicResource ThirdlyColor}"/>
		
		<SolidColorBrush x:Key="DefaultBrush" Color="{DynamicResource DefaultColor}"/>
		<SolidColorBrush x:Key="InfoBrush" Color="{DynamicResource InfoColor}"/>
		<SolidColorBrush x:Key="SuccessBrush" Color="{DynamicResource SuccessColor}"/>
		<SolidColorBrush x:Key="WarningBrush" Color="{DynamicResource WarningColor}"/>
		<SolidColorBrush x:Key="ErrorBrush" Color="{DynamicResource ErrorColor}"/>	
		<SolidColorBrush x:Key="DangerBrush" Color="{DynamicResource DangerColor}"/>

		<SolidColorBrush x:Key="Background" Color="{DynamicResource BackgroundColor}"/>
		<SolidColorBrush x:Key="Foreground" Color="{DynamicResource ForegroundColor}"/>

		<!--Disable-->
		<SolidColorBrush x:Key="DisableBackground" Color="{DynamicResource DisableBackgroundColor}"/>
		<SolidColorBrush x:Key="DisableForeground" Color="{DynamicResource DisableForegroundColor}"/>
		<SolidColorBrush x:Key="DisableBorderBrush" Color="{DynamicResource DisableBorderColor}"/>
	
		<!--Error-->
		<SolidColorBrush x:Key="ErrorBorderBrush" Color="{DynamicResource ErrorColor}"/>
		<SolidColorBrush x:Key="ErrorBackground" Color="{DynamicResource ErrorColor}"/>
		<SolidColorBrush x:Key="ErrorForeground" Color="{DynamicResource ErrorColor}"/>

		<!--Button-->
		<!--RepeatButton-->
		<SolidColorBrush x:Key="ButtonDefaultBackground" Color="{DynamicResource TransparentColor}"/>
		<SolidColorBrush x:Key="ButtonDefaultForeground" Color="{DynamicResource ButtonDefaultForegroundColor}"/>
		<SolidColorBrush x:Key="ButtonDefaultBorderBrush" Color="{DynamicResource ButtonDefaultBorderColor}"/>
		<SolidColorBrush x:Key="ButtonHoverDefaultForeground" Color="{DynamicResource ButtonHoverDefaultForegroundColor}"/>
		<SolidColorBrush x:Key="ButtonHoverDefaultBorderBrush" Color="{DynamicResource ButtonHoverDefaultBorderColor}"/>
		<SolidColorBrush x:Key="ButtonPressedDefaultForeground" Color="{DynamicResource ButtonPressedDefaultForegroundColor}"/>
		<SolidColorBrush x:Key="ButtonPressedDefaultBorderBrush" Color="{DynamicResource ButtonPressedDefaultBorderColor}"/>

		<SolidColorBrush x:Key="ButtonFullBackground" Color="{DynamicResource ButtonFullBackgroundColor}"/>
		<SolidColorBrush x:Key="ButtonFullForeground" Color="{DynamicResource ButtonFullForegroundColor}"/>
		<SolidColorBrush x:Key="ButtonHoverFullBackground" Color="{DynamicResource ButtonHoverFullBackgroundColor}"/>
		<SolidColorBrush x:Key="ButtonPressedFullBackground" Color="{DynamicResource ButtonPressedFullBackgroundColor}"/>

		<SolidColorBrush x:Key="ButtonDangerBrush" Color="{DynamicResource ButtonDangerColor}"/>
		<SolidColorBrush x:Key="ButtonDangerHoverBrush" Color="{DynamicResource ButtonDangerHoverColor}"/>
		<SolidColorBrush x:Key="ButtonDangerPressedBrush" Color="{DynamicResource ButtonDangerPressedColor}"/>
		
		<!--ScrollBar-->
		<SolidColorBrush x:Key="ScrollBarThumbBackground" Color="{DynamicResource ScrollBarThumbBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarThumbHoverBackground" Color="{DynamicResource ScrollBarThumbHoverBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarThumbPressedBackground" Color="{DynamicResource ScrollBarThumbPressedBackgroundColor}"/>

		<SolidColorBrush x:Key="ScrollBarLineButtonForeground" Color="{DynamicResource ScrollBarLineButtonForegroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarLineButtonHoverForeground" Color="{DynamicResource ScrollBarLineButtonHoverForegroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarLineButtonPressedForeground" Color="{DynamicResource ScrollBarLineButtonPressedForegroundColor}"/>

		<SolidColorBrush x:Key="ScrollBarLineButtonBackground" Color="{DynamicResource ScrollBarLineButtonBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarLineButtonHoverBackground" Color="{DynamicResource ScrollBarLineButtonHoverBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarLineButtonPressedBackground" Color="{DynamicResource ScrollBarLineButtonPressedBackgroundColor}"/>

		<SolidColorBrush x:Key="ScrollBarLineButtonBorderBrush" Color="{DynamicResource ScrollBarLineButtonBorderColor}"/>
		
		<SolidColorBrush x:Key="ScrollBarBackground" Color="{DynamicResource ScrollBarBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarForeground" Color="{DynamicResource ScrollBarForegroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarBorderBrush" Color="{DynamicResource ScrollBarBorderColor}"/>

		<SolidColorBrush x:Key="ScrollBarTrackBackground" Color="{DynamicResource ScrollBarTrackBackgroundColor}"/>
		<SolidColorBrush x:Key="ScrollBarTrackBorderBrush" Color="{DynamicResource ScrollBarTrackBorderColor}"/>

		<!--ScrollViewer-->
		<SolidColorBrush x:Key="ScrollViewerScrollBarsSeparatorBackground" Color="{DynamicResource ScrollViewerScrollBarsSeparatorBackgroundColor}"/>

		<!--TextBox-->
		<SolidColorBrush x:Key="TextControlCaretBrush" Color="{DynamicResource TextControlCaretColor}"/>
		<SolidColorBrush x:Key="TextControlBackground" Color="{DynamicResource TextControlBackgroundColor}"/>
		<SolidColorBrush x:Key="TextControlForeground" Color="{DynamicResource TextControlForegroundColor}"/>
		<SolidColorBrush x:Key="TextControlBorderBrush" Color="{DynamicResource TextControlBorderColor}"/>
		<SolidColorBrush x:Key="TextControlHoverBorderBrush" Color="{DynamicResource TextControlHoverBorderColor}"/>
		<SolidColorBrush x:Key="TextControlPlaceholderForeground" Color="{DynamicResource TextControlPlaceholderForegroundColor}"/>
		<SolidColorBrush x:Key="TextControlSelectionBackground" Color="{DynamicResource TextControlSelectionBackgroundColor}"/>
		<SolidColorBrush x:Key="TextControlSelectionForeground" Color="{DynamicResource TextControlSelectionForegroundColor}"/>
		<SolidColorBrush x:Key="TextControlFloatingForeground" Color="{DynamicResource TextControlFloatingForegroundColor}"/>

		<SolidColorBrush x:Key="TextControlButtonBackground" Color="{DynamicResource TextControlButtonBackgroundColor}"/>
		<SolidColorBrush x:Key="TextControlButtonForeground" Color="{DynamicResource TextControlButtonForegroundColor}"/>
		<SolidColorBrush x:Key="TextControlButtonHoverBackground" Color="{DynamicResource TextControlButtonHoverBackgroundColor}"/>
		<SolidColorBrush x:Key="TextControlButtonPressedBackground" Color="{DynamicResource TextControlButtonPressedBackgroundColor}"/>
		<SolidColorBrush x:Key="TextControlButtonBorderBrush" Color="{DynamicResource TextControlButtonBorderColor}"/>


		
		<!--ToolTip-->
		<SolidColorBrush x:Key="ToolTipBackground" Color="{DynamicResource ToolTipBackgroundColor}"/>
		<SolidColorBrush x:Key="ToolTipForeground" Color="{DynamicResource ToolTipForegroundColor}"/>
		<SolidColorBrush x:Key="ToolTipBorderBrush" Color="Transparent"/>



	</Style.Resources>

</Style>
