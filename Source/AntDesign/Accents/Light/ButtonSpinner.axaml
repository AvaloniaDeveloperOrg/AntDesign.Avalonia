<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="ButtonSpinnerBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ButtonSpinnerForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ButtonSpinnerBorderBrush" Color="{StaticResource AntDesigndAssistColor3}"/>

	<SolidColorBrush x:Key="ButtonSpinnerButtonBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonBorderBrush" Color="{StaticResource AntDesigndAssistColor3}"/>

	<SolidColorBrush x:Key="ButtonSpinnerButtonPointerOverBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonPointerOverForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonPointerOverBorderBrush" Color="{StaticResource AntDesignSecondlyColor}"/>

	<SolidColorBrush x:Key="ButtonSpinnerButtonPressedBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonPressedForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ButtonSpinnerButtonPressedBorderBrush" Color="{StaticResource AntDesignThirdlyColor}"/>

</ResourceDictionary>
