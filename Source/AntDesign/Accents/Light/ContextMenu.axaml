<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<BoxShadows x:Key="ContextMenuShadow">0 0 10 1 #4F000000</BoxShadows>

	<SolidColorBrush x:Key="ContextMenuBackground" Color="{StaticResource AntDesignBackgroundColor}"/>
	<SolidColorBrush x:Key="ContextMenuForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ContextMenuBorderBrush" Color="Transparent"/>
	
</ResourceDictionary>
