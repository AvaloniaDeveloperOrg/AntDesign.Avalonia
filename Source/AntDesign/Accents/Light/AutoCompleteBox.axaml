<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="AutoCompleteBoxBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="AutoCompleteBoxForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="AutoCompleteBoxBorderBrush" Color="{StaticResource AntDesigndAssistColor3}"/>

	<SolidColorBrush x:Key="AutoCompleteBoxPointerOverBorderBrush" Color="{StaticResource AntDesignSecondlyColor}"/>
	
	<SolidColorBrush x:Key="AutoCompleteBoxFocusBorderBrush" Color="{StaticResource AntDesignThirdlyColor}"/>

	<SolidColorBrush x:Key="AutoCompleteBoxFloatingForeground" Color="{StaticResource AntDesignSecondlyColor}"/>

	<SolidColorBrush x:Key="AutoCompleteBoxPopupBackground" Color="{StaticResource AntDesignBackgroundColor}"/>
	<SolidColorBrush x:Key="AutoCompleteBoxPopupBorderBrush" Color="{StaticResource AntDesigndAssistColor3}"/> 
	
</ResourceDictionary>
