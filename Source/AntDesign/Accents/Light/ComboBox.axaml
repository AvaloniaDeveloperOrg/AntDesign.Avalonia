<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="ComboBoxBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="ComboBoxForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ComboBoxBorderBrush" Color="Transparent"/>

	<SolidColorBrush x:Key="ComboBoxPointerOverBackground" Color="{StaticResource AntDesignAssistColor2}"/>
	<SolidColorBrush x:Key="ComboBoxPointerOverForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ComboBoxPointerOverBorderBrush" Color="Transparent"/>

	<SolidColorBrush x:Key="ComboBoxPressedBackground" Color="{StaticResource AntDesigndAssistColor5}"/>
	<SolidColorBrush x:Key="ComboBoxPressedForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="ComboBoxPressedBorderBrush" Color="Transparent"/>

	<SolidColorBrush x:Key="ComboBoxTiggerStatusForeground" Color="Transparent"/>
	<SolidColorBrush x:Key="ComboBoxPlaceHolderForeground" Color="Transparent"/>

</ResourceDictionary>
