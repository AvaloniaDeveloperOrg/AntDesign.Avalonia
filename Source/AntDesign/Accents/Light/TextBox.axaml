<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:CompileBindings="True">

	<SolidColorBrush x:Key="TextBoxBackground" Color="Transparent"/>
	<SolidColorBrush x:Key="TextBoxForeground" Color="{StaticResource AntDesignForegroundColor}"/>
	<SolidColorBrush x:Key="TextBoxCaretBrush" Color="Black"/>
	<SolidColorBrush x:Key="TextBoxBorderBrush" Color="#d9d9d9"/>
	<SolidColorBrush x:Key="TextBoxSelectionBackground" Color="#3390ff"/>
	<SolidColorBrush x:Key="TextBoxSelectionForeground" Color="White"/>

	<SolidColorBrush x:Key="TextBoxPointerOverBorderBrush" Color="{DynamicResource AntDesignSecondlyColor}"/>
	<SolidColorBrush x:Key="TextBoxPointerFocusBorderBrush" Color="{DynamicResource AntDesignThirdlyColor}"/>
	
	<SolidColorBrush x:Key="TextBoxFloatingForeground" Color="#4096ff"/>

	<SolidColorBrush x:Key="TextBoxButtonBackground" Color="#a6a6a6"/>
	<SolidColorBrush x:Key="TextBoxButtonForeground" Color="{DynamicResource AntDesignAssistColor1}"/>
	<SolidColorBrush x:Key="TextBoxButtonForeground1" Color="{DynamicResource AntDesigndAssistColor4}"/>
	<SolidColorBrush x:Key="TextBoxButtonBorderBrush" Color="Transparent"/>

	<SolidColorBrush x:Key="TextBoxButtonPointerOverBackground" Color="#8c8c8c"/>
	
	<SolidColorBrush x:Key="TextBoxButtonPressedBackground" Color="#8c8c8c"/>

	<!-- ShadowTextBooxStyle -->
	<BoxShadows x:Key="TextBoxShadow">0 0 4 2 #9f40a9ff</BoxShadows>

</ResourceDictionary>
